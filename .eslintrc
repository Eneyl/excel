{
  "parser": "babel-eslint",
  
 
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "amd": true,
    
  },
  "extends": ["eslint:recommended", "google"],
  "rules": {
    "arrow-parens": "off",
    "comma-dangle": "off",
    "object-curly-spacing": "off",
    "require-jsdoc": "off",
    "linebreak-style": [2, " windows "]
  },
}
